<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gameproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".puzzle_game.puzzleGameActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".puzzle_game.PuzzleGameIntroActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".reaction_game.reactionGameMain"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".reaction_game.reactionGameActivity"
            android:parentActivityName=".obstacle_game.ObstacleGameIntroActivity" />
        <activity
            android:name=".obstacle_game.ObstacleGameIntroActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".obstacle_game.ObstacleGameActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".obstacle_game.ObstacleGameEndActivity"
            android:parentActivityName=".obstacle_game.ObstacleGameIntroActivity" />
        <activity android:name=".SignUpActivity"
            android:parentActivityName=".LoginActivity" />
        <activity android:name=".puzzle_game.ImageActivity"
            android:parentActivityName=".puzzle_game.PuzzleGameIntroActivity" />
    </application>

</manifest>